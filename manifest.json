{
  "manifest_version": 2,

  "name": "Google Cloud Messaging Tester Client",
  "description": "This extension can receive messages from an through Google Cloud Messaging.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "gcm",
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/",
    "http://localhost:8080/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}